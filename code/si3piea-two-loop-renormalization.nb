Cell[CellGroupData[{

Cell["\<\
Gather divergences proportional v, tfing and tfinn and set independently to \
zero\
\>", "Subsection",
 CellChangeTimes->{{3.63002612899226*^9, 3.63002616209772*^9}}],

Cell["\<\
First we subtract the finite equation of motion, then gather coefficients of \
the remainder into a list and set each to zero (after some trimming and \
simplifying).\
\>", "Text",
 CellChangeTimes->{{3.631945244447894*^9, 3.6319453097587843`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"cteq", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"CoefficientList", "[", 
           RowBox[{
            RowBox[{"mg2soln", "+", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", 
                SuperscriptBox["m", "2"]}], "-", 
               RowBox[{
                FractionBox["\[Lambda]", "6"], 
                SuperscriptBox["v", "2"]}], "-", 
               RowBox[{
                FractionBox["\[HBar]", "6"], 
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"n", "+", "1"}], ")"}], "\[Lambda]"}], ")"}], 
                RowBox[{"(", "tfing", ")"}]}], "-", 
               RowBox[{
                FractionBox["\[HBar]", "6"], 
                RowBox[{"(", "\[Lambda]", ")"}], 
                RowBox[{"(", "tfinn", ")"}]}]}], ")"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"p", ",", "v", ",", "tfing", ",", "tfinn"}], "}"}]}], 
           "]"}], "//", "Flatten"}], ")"}], "//", "DeleteDuplicates"}], "//", 
       "Simplify"}], "//", "FullSimplify"}], ")"}], "\[Equal]", "0"}], "//", 
   "Thread"}]}]], "Input",
 CellChangeTimes->{{3.630024801628667*^9, 3.630024810908495*^9}, {
   3.630025128947822*^9, 3.630025151352537*^9}, {3.630025202227331*^9, 
   3.6300252032247143`*^9}, {3.630025296739283*^9, 3.630025303945628*^9}, 
   3.6300257621847754`*^9, {3.630103899530512*^9, 3.63010390017029*^9}, {
   3.630718992894826*^9, 3.630718993092227*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"-", 
     RowBox[{"(", 
